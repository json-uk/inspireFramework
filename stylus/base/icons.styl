// Icons
// ============================================

// 

@font-face {
  font-family: 'inspiredGridIcons';
  src:url('../../stylus/fonts/inspiredGridIcons.eot');
  src:url('../../stylus/fonts/inspiredGridIcons.eot?#iefix') format('embedded-opentype'), url('../../stylus/fonts/inspiredGridIcons.woff') format('woff'), url('../../stylus/fonts/inspiredGridIcons.ttf') format('truetype'), url('../../stylus/fonts/inspiredGridIcons.svg#inspiredGridIcons') format('svg');
  font-weight: normal;
  font-style: normal;
}

/* Use the following CSS code if you want to use data attributes for inserting your icons */
[data-icon]:before {
  font-family: 'inspiredGridIcons';
  content: attr(data-icon);
  speak: none;
  /* Enable Ligatures */
  -webkit-font-feature-settings:"liga","dlig";-moz-font-feature-settings:"liga=1, dlig=1";-moz-font-feature-settings:"liga","dlig";-ms-font-feature-settings:"liga","dlig";-o-font-feature-settings:"liga","dlig";
  font-feature-settings:"liga","dlig";
  text-rendering:optimizeLegibility;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  line-height: inherit;
  vertical-align: baseline;
}



